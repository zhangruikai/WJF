        android:theme="@style/AppTheme"
        >
        <activity android:name=".base.actity.MainActivity"
            android:theme="@style/MyAppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".base.actity.Viewpager_GuilderPage"></activity>

        <activity android:name=".base.actity.Login_main_page"></activity>

        <activity android:name=".base.actity.SendcodeActivity"></activity>

        <activity android:name=".base.actity.Register_Activity"></activity>

        <activity android:name=".base.actity.Sufferlly_actity"></activity>

    </application>

</manifest>